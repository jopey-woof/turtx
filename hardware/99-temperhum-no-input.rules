# TEMPerHUM Sensor - Prevent Input Interference
# This rule prevents the TEMPerHUM sensor from being treated as a keyboard input device

# TEMPerHUM USB sensor (vendor ID 0x3553, product ID 0xa001)
SUBSYSTEM=="hidraw", ATTRS{idVendor}=="3553", ATTRS{idProduct}=="a001", MODE="0666", GROUP="dialout", ENV{ID_INPUT}="0", ENV{ID_INPUT_KEYBOARD}="0"

# Also prevent input for the USB device itself
SUBSYSTEM=="usb", ATTRS{idVendor}=="3553", ATTRS{idProduct}=="a001", ENV{ID_INPUT}="0", ENV{ID_INPUT_KEYBOARD}="0"

# Prevent any HID device with TEMPerHUM characteristics from being treated as input
SUBSYSTEM=="hidraw", KERNEL=="hidraw*", ATTRS{idVendor}=="3553", ENV{ID_INPUT}="0", ENV{ID_INPUT_KEYBOARD}="0", ENV{ID_INPUT_MOUSE}="0"

# Additional rule to prevent input events from being generated
SUBSYSTEM=="input", ATTRS{idVendor}=="3553", ATTRS{idProduct}=="a001", ENV{ID_INPUT}="0", ENV{ID_INPUT_KEYBOARD}="0" 